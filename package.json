{
  "name": "opya",
  "productName": "opya",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "prod": "webpack --config webpack.prod.js",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:8080 && electron-forge start\"",
    "dev": "webpack-dev-server -d --content-base ./src/public --config webpack.dev.js ",
    "package": "electron-forge package",
    "make": "ELECTRON_IS_DEV=0 concurrently \"rm -f ./src/public/bundle.js && npm run prod\" \"wait-on ./src/public/bundle.js && electron-forge make\"",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "fherrero",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "linux": [
          "flatpak"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "ignore": [
          ".idea"
        ]
      },
      "electronWinstallerConfig": {
        "name": "opya"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "opya"
      }
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.0.0-alpha.6",
    "css-loader": "^0.28.7",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-is-dev": "^0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "*",
    "file-loader": "^1.1.5",
    "google-map-react": "^0.25.0",
    "image-webpack-loader": "^3.4.2",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "pubnub-react": "^1.2.0",
    "react": "*",
    "react-dom": "*",
    "react-places-autocomplete": "^5.4.3",
    "react-redux": "*",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-table": "^6.7.4",
    "recompose": "^0.26.0",
    "redux": "*",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-loader": "*",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "*",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.1",
    "electron-forge": "^5.1.1",
    "electron-prebuilt-compile": "1.8.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "wait-on": "^2.1.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.2"
  }
}
